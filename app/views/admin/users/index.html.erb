<h2>Admin User List</h2>


<h2></h2>
<div class="row">
  <div class="col-sm-12">


    <table class="table table-striped table-sm">
      <tr>
        <th>ID</th>
        <th>用户名</th>
        <th></th>
      </tr>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.username %></td>
          <td></td>
        </tr>
      <% end -%>
    </table>

    <%= will_paginate @users %>
  </div>
</div>